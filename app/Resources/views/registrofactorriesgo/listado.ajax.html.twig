<table id="factorriesgo_listado" class="table  table-bordered table-striped table-hover table-condensed">
    <thead>
        <tr>
            <th class="text-center col-md-3">
                <a href="#" onclick="regfactorriesgoNew();" class="btn btn-orange"><i class="fa fa-plus" aria-hidden="true"></i> Nuevo</a>
            </th>
            <th class="text-center">Nombre</th>
        </tr>
    </thead>
    <tbody>
        {% if  regfactorriesgo  is not  defined or regfactorriesgo | length == 0 %}
            <tr>
                <td class="text-center" colspan="2">
                    <i>(vac&iacute;o)</i>
                </td>
            </tr>
        {% else %}
            {% for regfr in regfactorriesgo  %}
                <tr>
                    <td class="text-center">
                        <a href="#" onclick="regfactorriesgoView('{{ path('regfactorriesgo_show', { 'id': regfr.id }) }}');"  class="btn btn-xs  btn-primary " data-placement="top" data-original-title="Ver registro de enfermedad">
                            <i class="fa fa-binoculars" aria-hidden="true"></i>
                        </a>
                        <a href="#" onclick="regfactorriesgoDelete('{{ path('regfactorriesgo_delete', { 'id': regfr.id }) }}');"  class="btn btn-xs  btn-danger " data-placement="top" data-original-title="Ver registro de enfermedad">
                            <i class="fa fa-trash" aria-hidden="true"></i>
                        </a>
                    </td>
                    <td class="text-center">{{ regfr.factorRiesgo.nombre }}</td>
                </tr>
            {% endfor %}
        {% endif %}
    </tbody>
</table>